# ls aliases
alias ls='ls --time-style=long-iso'
alias ll='ls -alh

# find aliases
alias ff='find . -type f -name'
alias fd='find . -type d -name'

# apt aliases
alias apti="sudo apt-get install"
alias apts="sudo apt-cache search"
alias aptu="sudo apt-get update \
                && sudo apt-get upgrade"
